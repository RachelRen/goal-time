{
    "compilerOptions": {
        "outDir": "./static/build/",
        "sourceMap": true,
        "noImplicitAny": true,
        // "module": "es2015",
        "target": "es6",
        "jsx": "react",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./static/",
        "paths": {
            "Page": ["./static/src/page"],
        },
    },
    
    "include": [
        "./static/src/**/*"
    ]
}